{
  "title": "demo",
  "description": "demo",
  "language": "zh-cn",
  "plugins": [ 
    "disqus",
    "github",
    "prism",
    "highlight",
    "splitter",
    "tbfed-pagefooter",
    "fontsettings",
    "simple-page-toc",
    "links",
    "katex"
  ],
  "pluginsConfig": {
       "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 12
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "links": {
            "gitbook": false,
            "sharing": {
                "google": false,
                "facebook": false,
                "twitter": false,
                "all": false
            }
        },
    "disqus": {
      "shortName": "webpack-handbook"
    },
    "github": {
      "url": "https://github.com/webChatContact/baas-v2-help"
    }
  }
}